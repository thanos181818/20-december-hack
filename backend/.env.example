# Database connection string
# Railway will auto-provide this as DATABASE_URL when you add PostgreSQL
DATABASE_URL=postgresql+asyncpg://postgres:password@localhost:5433/appareldesk

# JWT Secret Key (generate a random string for production)
SECRET_KEY=your-super-secret-key-change-in-production

# Frontend URL for CORS (Railway will give you this after deploying frontend)
FRONTEND_URL=http://localhost:8080
